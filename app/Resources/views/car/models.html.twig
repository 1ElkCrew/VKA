{% extends 'base.html.twig' %}
{% block body %}
    <h1>Models</h1>
    <table class="table table-striped table-condensed table-bordered table-hover col-xs-10">
    <tbody>
    {% for m in models %}
        <tr>
            <td class="col-xs-1">
                {{ m.id }}
            </td>
            <td class="col-xs-11">
                <a href="{{ path("edit_models", {modelsType: m.id}) }}">
                    {{ m.name }}
                </a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
    </table>
{% endblock %}